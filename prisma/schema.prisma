// Prisma schema for Titcha - Supabase PostgreSQL connection
// This file configures the database connection and will hold data models

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// Note: Models will be added here as needed for the application
// For now, this schema establishes the connection to Supabase
